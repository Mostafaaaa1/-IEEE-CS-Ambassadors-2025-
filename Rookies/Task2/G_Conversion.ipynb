      "source": [
        "s = input()\n",
        "converted = \"\"\n",
        "for ch in s:\n",
        "    if ch == ',':\n",
        "        converted += ' '\n",
        "    elif ch.islower():\n",
        "        converted += ch.upper()\n",
        "    elif ch.isupper():\n",
        "        converted += ch.lower()\n",
        "    else:\n",
        "        converted += ch\n",
        "print(converted)"
      ],
